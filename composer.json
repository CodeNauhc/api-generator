{
    "name": "jobins/api-generator",
    "description": "Generate api docs while writing test case (Laravel).",
    "keywords": [
        "JoBins",
        "documentation",
        "api",
        "swagger",
        "openapi",
        "phpunit"
    ],
    "homepage": "https://github.com/JoBinsJP/api-genereator",
    "license": "MIT",
    "authors": [
        {
            "name": "Bedram Tamang",
            "email": "tmgbedu@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "JoBins\\APIGenerator\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "psalm": "vendor/bin/psalm",
        "pint": "vendor/bin/pint",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "style-check": "php-cs-fixer fix --dry-run --verbose --diff",
        "style-fix": "php-cs-fixer fix --verbose"
    },
    "autoload-dev": {
        "psr-4": {
            "JoBins\\APIGenerator\\Tests\\": "tests"
        },
        "files": [
            "tests/helper.php"
        ]
    },
    "require-dev": {
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^4.7",
        "friendsofphp/php-cs-fixer": "^3.14"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoBins\\APIGenerator\\APIGeneratorServiceProvider"
            ]
        }
    }
}
